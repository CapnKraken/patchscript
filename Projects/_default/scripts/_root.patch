Start
    def setcolor r g b
        setattribute _draw_r r
        setattribute _draw_g g
        setattribute _draw_b b
        setattribute _draw_a 255
        setvar _return [r, g, b, 255]
    return

    setattribute _draw_centered 1
    setattribute _draw_antialiased 1
    setattribute _draw_stroke 40
    setcolor r=255 g=255 b=255
    
    setposition 240 180

    draw _self text "Welcome to Patchscript!"  
End